  "license": "MIT",
  "dependencies": {
    "@types/uuid": "^8.3.4",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1204.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "compile": "tsc",
    "debug-lin": "export SLS_DEBUG=* && node --debug-port 9229  ./node_modules/.bin/serverless offline -s $SLS_STAGE",
    "debug-win": "SET SLS_DEBUG=* && node --debug-port 9229 %USERPROFILE%\\AppData\\Roaming\\npm\\node_modules\\serverless\\bin\\serverless offline -s %SLS_STAGE%",
    "vc-debug-li": "export SLS_DEBUG=* && node --inspect /usr/local/bin/serverless offline",
    "debug": "SET SLS_DEBUG=* && node --inspect node_modules\\serverless\\bin\\serverless offline"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.102",
    "@types/node": "^18.7.13",
    "serverless": "^3.22.0",
    "serverless-offline": "^8.5.0",
    "serverless-plugin-typescript": "^2.1.2",
    "typescript": "^4.8.2"
  }
}
